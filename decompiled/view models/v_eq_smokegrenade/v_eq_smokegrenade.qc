	fadeout 0.2
	blendlayer "throwcharge_blend" 27 30 39 39
	fps 41
}

$sequence "throw" {
	"v_eq_smokegrenade_anims\throw.smd"
	activity "ACT_VM_THROW" 1
	{ event 3005 10 "" }
	fadein 0.2
	fadeout 0.2
	snap
	fps 30
}

$sequence "deploy" {
	"v_eq_smokegrenade_anims\deploy.smd"
	activity "ACT_VM_DRAW" 1
	{ event 5004 0 "Flashbang.Draw" }
	fadein 0.2
	fadeout 0.2
	snap
	fps 30
}

$sequence "underhand" {
	"v_eq_smokegrenade_anims\underhand.smd"
	activity "ACT_VM_RELEASE" 1
	{ event 3005 10 "" }
	fadein 0.2
	fadeout 0.2
	snap
	fps 27
}

$sequence "throwcharge_blend" {
	"throwcharge_low"
	"throwcharge_mid"
	"throwcharge_high"
	blend "throwcharge" 0 1
	blendwidth 3
	fadein 0.2
	fadeout 0.2
}
