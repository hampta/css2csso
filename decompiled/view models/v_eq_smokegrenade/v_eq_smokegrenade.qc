	fps 30
}

$animation "throwcharge_low" "v_eq_smokegrenade_anims\throwcharge_low.smd" {
	fps 30
}

$sequence "idle" {
	"v_eq_smokegrenade_anims\idle.smd"
	activity "ACT_VM_IDLE" 1
	fadein 0.2
	fadeout 0.2
