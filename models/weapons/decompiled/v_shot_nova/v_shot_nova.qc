$sequence "idle" {
	"v_shot_nova_anims\idle.smd"
	activity "ACT_VM_IDLE" 1
	fadein 0.2
	fadeout 0.2
	fps 12
}

$sequence "shoot1" {
	"v_shot_nova_anims\shoot1.smd"
	activity "ACT_VM_PRIMARYATTACK" 1
	{ event 5001 0 "1" }
	{ event AE_CLIENT_EJECT_BRASS 22 "" }
	{ event AE_CLIENT_EFFECT_ATTACH 22 "EjectBrass_12Gauge 2 70" }
	fadein 0.2
	fadeout 0.2
	snap
	fps 32
}

$sequence "start_reload" {
	"v_shot_nova_anims\start_reload.smd"
	activity "ACT_SHOTGUN_RELOAD_START" 1
	fadein 0.2
	fadeout 0.2
	fps 40
}

$sequence "insert" {
	"v_shot_nova_anims\insert.smd"
	activity "ACT_VM_RELOAD" 1
	{ event 5004 0 "Weapon_Nova.Insertshell" }
	fadein 0.2
	fadeout 0.2
	fps 55
}

$sequence "after_reload" {
	"v_shot_nova_anims\after_reload.smd"
	activity "ACT_SHOTGUN_RELOAD_FINISH" 1
	{ event 5004 12 "Weapon_Nova.Pump" }
	fadein 0.2
	fadeout 0.2
	snap
	fps 40
}

$sequence "draw" {
	"v_shot_nova_anims\draw.smd"
	activity "ACT_VM_DRAW" 1
	{ event 5004 0 "Weapon_Nova.Draw" }
	{ event 5004 11 "Weapon_Nova.Pump" }
	fadein 0.2
	fadeout 0.2
	snap
	fps 30
}

$sequence "lookat01" {
	"v_shot_nova_anims\lookat01.smd"
	{ event AE_CL_SET_STATTRAK_GLOW 0 "1" }
	{ event AE_CL_SET_STATTRAK_GLOW 0 "0" }
	{ event AE_BEGIN_TAUNT_LOOP 0 "0.5714" }
	fadein 0.3
	fadeout 0.3
	fps 30
}

$sequence "lookat01_prepare" {
	"v_shot_nova_anims\lookat01_prepare.smd"
	fadein 0.2
	fadeout 0.2
	fps 30
}

$sequence "lookat01_loop" {
	"v_shot_nova_anims\lookat01_loop.smd"
	fadein 0.2
	fadeout 0.2
	fps 15
}
